<?xml version="1.0"?>
<object name="portal_workflow">
		<object name="emc_project_workflow" meta_type="Workflow"/>
		<object name="emc_doc_workflow" meta_type="Workflow"/>		
    <bindings>
    	<default/>
    	    <type type_id="emc.project.project">
            <bound-workflow workflow_id="emc_project_workflow"/>
        </type>
    	    <type type_id="emc.project.team">
            <bound-workflow workflow_id="emc_project_workflow"/>
        </type>
    	    <type type_id="emc.project.doc">
            <bound-workflow workflow_id="emc_doc_workflow"/>
        </type>
               
    	    <type type_id="emc.bokeh.fearture">
            <bound-workflow workflow_id="emc_project_workflow"/>
        </type>
    	    <type type_id="emc.bokeh.codefile">
            <bound-workflow workflow_id="emc_project_workflow"/>
        </type>                                        
    </bindings>
</object>